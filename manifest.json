{
    "name": "TwiHub",
    "version": "0.01",
    "description": "Share code",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage",
        "https://cidb.cf:21027/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://github.com/*",
                "*://*.github.com/*"
            ],
            "js": [
                "js/file_info.js",
                "js/line_entry.js",
                "js/file_entry.js",
                "js/file_collection.js",
                "js/popup.js",
                "js/rendering.js",
                "js/main.js"
            ]
        }
    ],
    "page_action": {
        "default_icon": {
            "16": "logo/logo_16.png",
            "32": "logo/logo_32.png",
            "48": "logo/logo_48.png",
            "128": "logo/logo_128.png"
        }
    },
    "manifest_version": 2
}
